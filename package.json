{
  "name": "@funbox/blueprinter-frontend",
  "version": "3.2.1",
  "description": "Replacement of Aglio library for rendering generated AST as HTML page.",
  "files": [
    "/bin",
    "/static"
  ],
  "bin": {
    "blueprinter-frontend": "./bin/index.js"
  },
  "scripts": {
    "start": "npm run dev",
    "build": "npm run make-stub && funbox-build",
    "build-production": "funbox-build-production",
    "build-static": "node ./bin/build-static.js",
    "dev": "npm-run-all --parallel watch-stub start-dev",
    "start-dev": "funbox-dev",
    "make-stub": "node ./scripts/generate-stub.js",
    "watch-stub": "node ./scripts/watch-stub.js",
    "lint:scss": "stylelint \"src/**/*.scss\" --syntax scss --fix --config .stylelintrc --cache",
    "lint:js": "eslint --cache -c .eslintrc.js --ext .jsx,.js src config",
    "lint": "npm run -s lint:scss && npm run -s lint:js",
    "prebuild": "npm run lint",
    "prepublishOnly": "npm run build-static",
    "postpublish": "bash ./build-docker.sh"
  },
  "dependencies": {
    "@funbox/commonmark": "0.29.0-fb-1.0.2",
    "@funbox/crafter": "1.34.2",
    "@funbox/free-port-finder": "2.0.1",
    "@hot-loader/react-dom": "16.10.2",
    "browser-sync": "2.26.7",
    "react-hot-loader": "4.12.15",
    "yargs": "12.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/plugin-transform-react-constant-elements": "7.6.3",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-react": "7.6.3",
    "@funbox/eslint-config": "4.14.0",
    "@funbox/frontend-env-webpack": "13.4.0",
    "@funbox/scss-imports-loader": "2.1.0",
    "@funbox/scss-lint-config": "2.6.0",
    "@funbox/scss-utils": "1.1.1",
    "@funbox/scss-vars-loader": "2.1.0",
    "acorn": "6.4.0",
    "acorn-jsx": "5.1.0",
    "autoprefixer": "9.6.1",
    "babel-plugin-transform-react-remove-prop-types": "0.4.12",
    "base64-inline-loader": "1.1.1",
    "bem-react-helper": "1.1.2",
    "chokidar": "3.0.2",
    "core-js": "3.4.5",
    "css-loader": "0.28.9",
    "csso-webpack-plugin": "2.0.0-beta.1",
    "deep-equal": "1.0.1",
    "eslint": "5.16.0",
    "html-react-parser": "0.4.6",
    "html-webpack-inline-source-plugin": "0.0.10",
    "lodash": "4.17.15",
    "mini-css-extract-plugin": "0.8.0",
    "node-sass": "4.12.0",
    "npm-run-all": "4.1.5",
    "postcss-loader": "2.0.10",
    "postcss-url": "7.3.0",
    "prop-types": "15.6.0",
    "re-resizable": "5.0.0-beta.0",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-highlight": "0.12.0",
    "react-router-dom": "4.2.2",
    "react-slide-toggle": "0.1.13",
    "sass-loader": "6.0.6",
    "style-loader": "0.20.1",
    "stylelint": "8.4.0",
    "suppress-chunks-webpack-plugin": "0.0.4",
    "uniqid": "5.0.3",
    "url-parse": "1.4.7"
  },
  "pugLintConfig": {
    "extends": "funbox"
  }
}
