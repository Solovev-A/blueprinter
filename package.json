{
  "name": "@funbox/blueprinter-frontend",
  "version": "3.50.0",
  "description": "Replacement of Aglio library for rendering generated AST as HTML page.",
  "files": [
    "/bin",
    "/static"
  ],
  "bin": {
    "blueprinter-frontend": "./bin/index.js"
  },
  "scripts": {
    "start": "npm run dev",
    "build": "npm run make-stub && BABEL_ENV=production webpack --config ./config/webpack/build.config.js",
    "build-production": "BABEL_ENV=production webpack --config ./config/webpack/build.production.config.js",
    "build-static": "BABEL_ENV=production node ./bin/build-static.js",
    "dev": "npm-run-all --parallel watch-stub start-dev",
    "start-dev": "webpack-dev-server --progress --config ./config/webpack/dev.config.js",
    "make-stub": "node ./scripts/generate-stub.js",
    "watch-stub": "node ./scripts/watch-stub.js",
    "lint:scss": "stylelint \"src/**/*.scss\" --syntax scss --fix --config .stylelintrc --cache",
    "lint:js": "eslint --cache -c .eslintrc.js --ext .jsx,.js src config",
    "lint:test": "eslint -c tests/.eslintrc tests",
    "lint": "npm run -s lint:scss && npm run -s lint:js && npm run -s lint:test",
    "prebuild": "npm run lint",
    "prepublishOnly": "npm run build-static",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@funbox/commonmark": "0.29.0-fb-1.0.3",
    "@funbox/crafter": "2.24.0",
    "@funbox/free-port-finder": "2.0.1",
    "browser-sync": "2.26.14",
    "react-hot-loader": "4.12.19",
    "yargs": "15.4.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/plugin-transform-react-constant-elements": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@babel/preset-react": "7.8.3",
    "@funbox/eslint-config": "4.14.0",
    "@funbox/scss-imports-loader": "2.1.0",
    "@funbox/scss-lint-config": "2.7.0",
    "@funbox/scss-utils": "1.1.1",
    "@funbox/scss-vars-loader": "2.1.0",
    "@hot-loader/react-dom": "16.11.0",
    "@svgr/webpack": "5.5.0",
    "@types/react": "16.9.20",
    "autoprefixer": "9.7.4",
    "babel-loader": "8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "base64-inline-loader": "1.1.1",
    "bem-react-helper": "1.1.2",
    "chokidar": "3.3.1",
    "clean-webpack-plugin": "3.0.0",
    "core-js": "3.6.4",
    "css-loader": "3.4.2",
    "csso-webpack-plugin": "2.0.0-beta.1",
    "deep-equal": "2.0.1",
    "eslint": "5.16.0",
    "file-loader": "5.0.2",
    "html-react-parser": "0.10.1",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "3.2.0",
    "jest": "26.6.3",
    "jest-junit": "12.0.0",
    "mini-css-extract-plugin": "0.9.0",
    "npm-run-all": "4.1.5",
    "postcss-loader": "3.0.0",
    "postcss-url": "8.0.0",
    "prop-types": "15.7.2",
    "re-resizable": "6.2.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-highlight": "0.13.0",
    "react-router-dom": "5.1.2",
    "react-slide-toggle": "0.3.3",
    "react-window": "1.8.5",
    "resolve-url-loader": "3.1.2",
    "sass": "1.25.0",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.3",
    "stylelint": "13.2.0",
    "suppress-chunks-webpack-plugin": "1.0.6",
    "uniqid": "5.2.0",
    "url-parse": "1.4.7",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "4.2.2"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "tests"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "coverageReporters": [
      "html",
      "text",
      "cobertura"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ]
  }
}
