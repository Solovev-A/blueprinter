{
  "name": "@funbox/blueprinter-frontend",
  "version": "1.3.4",
  "description": "Replacement of Aglio library for rendering generated AST as HTML page.",
  "files": [
    "/bin",
    "/static"
  ],
  "scripts": {
    "start": "npm run dev",
    "build": "funbox-build",
    "build-production": "funbox-build-production",
    "build-static": "node ./bin/build-static.js",
    "dev": "funbox-dev",
    "lint:sass": "sass-lint -c node_modules/sass-lint-config-funbox/lib/.sass-lint.yml \"src/**/*.sass\" -v",
    "lint:scss": "stylelint \"src/**/*.scss\" --syntax scss --fix --config .stylelintrc --cache",
    "lint:js": "eslint --cache -c .eslintrc.js --ext .jsx,.js src config",
    "lint": "npm run -s lint:scss && npm run -s lint:sass && npm run -s lint:js",
    "prebuild": "npm run lint",
    "prepublishOnly": "npm run build-static"
  },
  "dependencies": {
    "browser-sync": "2.24.7",
    "funbox-free-port-finder": "1.2.0",
    "@funbox/protagonist": "1.6.8-fb-2.0.0",
    "yargs": "12.0.1"
  },
  "devDependencies": {
    "@babel/polyfill": "7.0.0-beta.51",
    "@funbox/eslint-config": "4.12.0",
    "autoprefixer": "7.2.5",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.12",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "base64-inline-loader": "1.1.1",
    "bem-react-helper": "1.1.2",
    "css-loader": "0.28.9",
    "csso-webpack-plugin": "1.0.0-beta.10",
    "deep-equal": "1.0.1",
    "eslint": "5.8.0",
    "extract-text-webpack-plugin": "2.1.2",
    "funbox-frontend-env-webpack": "10.2.0",
    "funbox-scss-imports-loader": "1.0.0",
    "funbox-scss-lint-config": "1.0.2",
    "funbox-scss-vars-loader": "1.0.0",
    "html-react-parser": "0.4.6",
    "html-webpack-inline-source-plugin": "0.0.10",
    "lodash": "4.17.4",
    "node-sass": "4.7.2",
    "postcss-loader": "2.0.10",
    "postcss-url": "7.3.0",
    "prop-types": "15.6.0",
    "re-resizable": "4.7.1",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-highlight": "0.12.0",
    "react-router-dom": "4.2.2",
    "react-slide-toggle": "0.1.13",
    "@funbox/sass-lint": "1.10.2-fb-2.0.1",
    "sass-lint-config-funbox": "1.4.1",
    "sass-loader": "6.0.6",
    "showdown": "1.8.6",
    "style-loader": "0.20.1",
    "stylelint": "8.4.0",
    "suppress-chunks-webpack-plugin": "0.0.4",
    "uniqid": "5.0.3",
    "url-parse": "1.4.1"
  },
  "pugLintConfig": {
    "extends": "funbox"
  }
}
